# Tmux
tmw() {
    if [ -z "$1" ]
    then
        tmux neww -n "$1"
        exit 1
    else
        tmux neww
    fi
}

tkill() {
    if [ -z "$1" ]; then
        tmux kill-ses -t "$1"
    else
        tmux kill-session -a
    fi
}

newproject() {
    NEW_PROJECT_DIR=~/Workspace/"$1"/

    mkdir -p $NEW_PROJECT_DIR
    cd $NEW_PROJECT_DIR
}
